{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "post-designer/carousel",
  "version": "0.1.0",
  "title": "Carousel",
  "category": "post-designer",
  "icon": "smiley",
  "description": "Design any kind of Posts",
  "supports": {
    "html": false
  },
  "textdomain": "blocky",
  "attributes": {
    "postType": {
      "type": "string",
      "default": "post"
    },
    "author": {
      "type": "string",
      "default": ""
    },
    "categories": {
      "type": "string",
      "default": ""
    },
    "tags": {
      "type": "string",
      "default": ""
    },
    "dateFrom": {
      "type": "string",
      "default": ""
    },
    "dateTo": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "post-designer/list",
	"version": "0.1.0",
	"title": "Post Lists",
	"category": "post-designer",
	"icon": "smiley",
	"description": "Design any kind of Posts",
	"supports": {
		"html": false
	},
	"textdomain": "blocky",
	"attributes": {
		"postType": {
			"type": "string",
			"default" : "post"
		},
		"postPerPage": {
			"type": "number",
			"default": 10
		},
		"noPagination": {
			"type": "boolean",
			"default": false
		},
		"orderBy": {
			"type": "string",
			"default": "ID"
		},
		"order": {
			"type": "string",
			"default": "DESC"
		},
		"taxonomies": {
			"type": "array",
			"default": []
		},
		"terms": {
			"type": "string",
			"default": []
		},
		"authors": {
			"type": "string",
			"default": []
		},
		"dateFrom": {
			"type": "string",
			"default": ""
		},
		"dateTo": {
			"type": "string",
			"default": ""
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
